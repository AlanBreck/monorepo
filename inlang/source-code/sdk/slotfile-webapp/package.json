{
	"name": "vite-vanilla-ts",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"postinstall": "npm run postinstall:logo",
		"postinstall:logo": "copyfiles ../../docs-src/files/logo/logo.svg src/ --flat --up",
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"test": "npm run build && concurrently \"npm run preview\" \"npm run test:e2e\" --kill-others --success first",
		"test:e2e": "testcafe chrome -e test/ --hostname localhost --selector-timeout 8000 --retry-test-pages",
		"proxy": "node ./proxy/index.js"
	},
	"devDependencies": {
		"async-test-util": "2.5.0",
		"concurrently": "8.2.2",
		"copyfiles": "2.4.1",
		"testcafe": "3.6.0",
		"typescript": "5.4.5",
		"vite": "5.2.12",
		"vite-plugin-top-level-await": "1.4.1"
	},
	"dependencies": {
		"@esbuild-plugins/node-globals-polyfill": "^0.2.3",
		"@esbuild-plugins/node-modules-polyfill": "^0.2.2",
		"@isomorphic-git/cors-proxy": "^2.7.1",
		"@lix-js/client": "^2.2.0",
		"@inlang/message-bundle-component": "workspace:*",
		"@inlang/sdk": "workspace:^",
		"buffer": "^6.0.3",
		"express": "^4.19.2",
		"isomorphic-git": "^1.25.10",
		"memfs": "4.6.0",
		"process": "^0.11.10",
		"readable-stream": "^4.5.2",
		"rxdb": "^15.24.0",
		"rxjs": "7.8.1",
		"stream": "^0.0.2",
		"vite-plugin-node-polyfills": "^0.22.0",
		"web": "link:isomorphic-git/http/web"
	}
}
