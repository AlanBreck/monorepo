{
	"name": "comlink-node",
	"description": "A utility package for isomorphic node & web workers with comlink",
	"type": "module",
	"author": "Loris Sigrist",
	"version": "0.0.1",
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"vitest": "./vitest.js",
			"types": "./types.d.ts",
			"node": "./node.js",
			"import": "./not-node.js"
		},
		"./worker": {
			"vitest": "./worker/vitest.js",
			"types": "./worker/types.d.ts",
			"node": "./worker/node.js",
			"import": "./worker/not-node.js"
		}
	},
	"dependencies": {
		"comlink": "^4.4.1"
	}
}
